# .gitignore for Flutter + Firebase Projects

# Miscellaneous operating system, editor, and temporary files
# =============================================================
# General
.DS_Store                   # macOS specific folder metadata
.Trash-*                    # Linux trash folder
*.log                       # General log files
.svn/                       # SVN version control directories
.buildlog/                  # Build log directory

# macOS specific
.AppleDouble
.LSOverride
.DocumentRevisions-V100
.fseventsd
.Spotlight-V100
.TemporaryItems
.Trashes
.VolumeIcon.icns
.com.apple.timemachine.donotpresent
.apdisk
Icon?
._*

# Linux/Unix specific
.fuse_hidden*               # FUSE temporary files
.directory                  # KDE directory preferences
.nfs*                       # NFS temporary files

# Vim / Emacs
*~                          # Editor backup files
# Swap files
[._]*.s[a-v][a-z]
[._]*.sw[a-p]
[._]s[a-v][a-z]
[._]sw[a-p]
# Session files
Session.vim
# Temporary & Tag files
.netrwhist
tags
# Persistent undo
[._]*.un~

# Other Editors
.atom/                      # Atom editor specific files
.history                    # VS Code Local History extension data


# IntelliJ / Android Studio related files
# =======================================
# Project and workspace files
*.iml                       # IntelliJ module files
*.ipr                       # IntelliJ project files
*.iws                       # IntelliJ workspace files
.idea/                      # IntelliJ/Android Studio project configuration directory

# User-specific settings (should not be committed)
.idea/**/workspace.xml
.idea/**/tasks.xml
.idea/**/dictionaries
.idea/**/shelf
.idea/**/assetWizardSettings.xml
.idea/**/caches

# Sensitive data (e.g., database connections, secrets)
.idea/**/dataSources/
.idea/**/dataSources.ids
.idea/**/dataSources.local.xml
.idea/**/sqlDataSources.xml
.idea/**/dynamic.xml

# UI and Plugin settings
.idea/**/uiDesigner.xml
.idea/**/dbnavigator.xml
.idea/**/mongoSettings.xml
.idea/replstate.xml         # Cursive Clojure plugin
.idea/httpRequests          # Editor-based Rest Client requests

# Generated files and build outputs
.idea/**/gradle.xml
.idea/**/libraries
.idea_modules/              # mpeltonen/sbt-idea plugin
cmake-build-debug/
cmake-build-release/
out/

# Plugin specific files
atlassian-ide-plugin.xml    # JIRA plugin
com_crashlytics_export_strings.xml # Crashlytics plugin files
crashlytics.properties
crashlytics-build.properties
fabric.properties


# VS Code related
# =================
# The .vscode folder contains launch configurations and tasks you may wish to
# share with your team. This is commented out by default. If you prefer to
# ignore all VS Code settings, uncomment the line below.
.vscode/


# Flutter / Dart / Pub related
# ============================
.dart_tool/                  # Dart toolchain build outputs and caches
.flutter-plugins             # Old Flutter plugin registration file
.flutter-plugins-dependencies # Old Flutter plugin dependency file
.packages                    # Generated by Dart/Flutter for package resolution
.pub/                        # Pub cache directory within the project
/build/                      # Main Flutter build output directory
**/doc/api/                  # Generated Dart documentation

# For applications, you should always check in pubspec.lock to ensure
# consistent builds for all developers and CI systems.
# Only uncomment the line below if you are building a reusable Dart package.
# pubspec.lock


# Android specific ignores
# ========================
# Built application files
*.apk
*.ap_
# Files for the ART/Dalvik VM
*.dex
# Java class files
*.class
# Generated files
bin/
gen/
# Gradle files
.gradle/
# Local configuration file (contains SDK path and other sensitive info)
local.properties
# Proguard folder generated by Eclipse
proguard/
# Android Studio captures folder
captures/
# Android Studio Navigation editor temp files
.navigation/
# External native build folder
.externalNativeBuild/
externalNativeBuild/
# Auto-generated Flutter plugin registrant
**/android/**/GeneratedPluginRegistrant.java


# --- Android Firebase and Signing Keys (CRITICAL) ---
# Ensure these are NEVER committed to public repositories.
# ====================================================
android/app/google-services.json # Firebase client API key & project identifiers.
android/key.properties           # Contains paths/passwords to app signing keystores.
GoogleService-Info.plist
*.jks                            # Java Keystore files (for app signing).
*.keystore                       # Generic keystore files (e.g., debug.keystore).

# firebase stuff from flutterfire
firebase.json
lib/firebase_options.dart

